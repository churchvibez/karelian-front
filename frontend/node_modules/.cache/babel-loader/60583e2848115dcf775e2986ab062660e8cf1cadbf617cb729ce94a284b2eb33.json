/website/frontend/src/pages/About.jsx"],"sourcesContent":["import React, { useState, useEffect } from 'react';\r\nimport { aboutUrlTitle, aboutUrlText } from '../apiConfig';\r\n\r\nconst About = () => {\r\n  const [aboutTitle, setAboutTitle] = useState('');\r\n  const [aboutText, setAboutText] = useState('');\r\n\r\n  useEffect(() => {\r\n    fetch(aboutUrlTitle)\r\n      .then(response => response.json())\r\n      .then(data => {\r\n        if (Array.isArray(data) && data.length > 0) {\r\n          setAboutTitle(data[0].a_title);\r\n        }\r\n      })\r\n      .catch(error => console.error('Error fetching data:', error));\r\n\r\n      fetch(aboutUrlText)\r\n      .then(response => response.json())\r\n      .then(data => {\r\n        if (Array.isArray(data) && data.length > 0) {\r\n          setAboutText(data[0].a_text);\r\n        }\r\n      })\r\n      .catch(error => console.error('Error fetching data:', error));\r\n\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"generalPages\">\r\n      <div>\r\n        <div>\r\n          <h2>О проекте</h2>\r\n        </div>\r\n        <div>\r\n          <p>Проект создания Интернет-ресурса &laquo;Карельский фронт. Безвозвратные потери частей и соединений Карельского фронта 1941 &ndash; 1944 гг.&raquo; выполнялся в 2009-2010 гг. научно-исследовательским и образовательным центром по проблемам Второй мировой войны на Севере Европы (НИОЦ ВМВСЕ)<a target=\"_blank\" href=\"http://petrsu.ru/Chairs/gen_hist.html\">кафедры всеобщей истории исторического факультета ПетрГУ</a> и сотрудниками ИФ ПетрГУ, занимающимися военно-исторической проблематикой.\r\n          <br/><br/>\r\n          <strong>Основная  цель проекта</strong>  &ndash; дать возможность  родным и близким погибших военнослужащих  самостоятельно найти места их захоронения или гибели. Проект адресован и всем интересующимся историей Великой Отечественной и Второй мировой войны.\r\n          <br/><br/>\r\n          Кроме того,  проект будет содержать большой массив данных о гражданском населении КФССР, как оставшемся на оккупированной территории, так и эвакуированном во внутренние районы СССР.\r\n          <br/><br/>\r\n          Интернет-ресурс на момент публикации (7.5.2010)  представляет собой исходную неполную версию, которая будет дополняться в ходе дальнейшей работы. Так, в базу данных на 7.5.2010 внесены сведения о 5 000 воинах Карельского фронта, захороненных на территории Республики Карелия из имеющихся записей на 51 000 человек.\r\n          <br/><br/>\r\n          <strong>Руководитель проекта:</strong>\r\n          <br/>\r\n          Д.и.н., профессор ПетрГУ <a target=\"_blank\" href=\"http://petrsu.ru/Chairs/GenHist/kilin.html\"><strong>Юрий Михайлович Килин</strong></a>\r\n          <br/><br/>\r\n          <strong>Исполнители: </strong>\r\n          <br/>\r\n          К.и.н., доцент <strong>Сергей Геннадьевич Веригин</strong>\r\n          <br/>\r\n          Специалист НИОЦ ВМВСЕ <strong>Вероника Владимировна Килина</strong>\r\n          <br/>\r\n          Доцент <strong>Ольга Анатольевна Киселева</strong>\r\n          <br/>\r\n          К.и.н., доцент <strong>Олег Игоревич Кулагин</strong>\r\n          <br/>\r\n          К.и.н., доцент <strong>Татьяна Владленовна Никулина</strong>\r\n          <br/><br/>\r\n          Исполнители проекта выражают глубокую признательность и благодарность за помощь в подготовке Интернет-ресурса ректору ПетрГУ профессору А.В.Воронину, сотрудникам РЦНИТ ПетрГУ Г.Н.Чимирко, Д.В.Денисову и Н.В.Вдовицыной.</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default About;\r\n"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,QAAQ,EAAEC,SAAS,QAAQ,OAAO;AAClD,SAASC,aAAa,EAAEC,YAAY,QAAQ,cAAc;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAE3D,MAAMC,KAAK,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAClB,MAAM,CAACC,UAAU,EAAEC,aAAa,CAAC,GAAGT,QAAQ,CAAC,EAAE,CAAC;EAChD,MAAM,CAACU,SAAS,EAAEC,YAAY,CAAC,GAAGX,QAAQ,CAAC,EAAE,CAAC;EAE9CC,SAAS,CAAC,MAAM;IACdW,KAAK,CAACV,aAAa,CAAC,CACjBW,IAAI,CAACC,QAAQ,IAAIA,QAAQ,CAACC,IAAI,